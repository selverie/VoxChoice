{
  "canisters": {
    "backend": {
      "type": "custom",
      "build": "npx azle backend",
      "root": "src",
      "main": "backend/index.ts",
      "candid": "backend/index.did",
      "wasm": ".azle/backend/backend.wasm",
      "gzip": true
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}
